{
  "name": "gradient",
  "version": "0.0.1",
  "description": "A block for creating color gradients",
  "main": "gradient.js",
  "scripts": {
    "test": "rollup -c rollup.config.test.js && tape dist/gradient.test.js",
    "build": "cp blocks/src/* blocks/dist && rollup -c -i blocks/src/gradient.js -o blocks/dist/gradient.js -f cjs"
  },
  "keywords": [
    "color", "material"
  ],
  "author": "Drew <drew@flux.io>",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015-rollup": "^1.1.1",
    "gl-matrix": "^2.3.2",
    "rollup": "^0.25.6",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-node-resolve": "^1.5.0",
    "tape": "^4.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flux-labs/gradient.git"
  },
  "bugs": {
    "url": "https://github.com/flux-labs/gradient/issues"
  },
  "homepage": "https://github.com/flux-labs/gradient#readme"
}
